{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "builds": [
        {
        "src": "dist/index.js", 
        "use": "@vercel/node@18",
        "config": {
            "maxDuration": 30
        }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/dist/index.js"
        }
    ]
}